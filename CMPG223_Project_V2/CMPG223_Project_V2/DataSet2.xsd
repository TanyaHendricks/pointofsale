            <MainSource>
              <DbSource ConnectionRef="CMPG223_DBV2ConnectionString (Settings)" DbObjectName="SELECT Sale_Header.Sale_Date, SUM(COALESCE (Sale_Detail.Sale_Detail_Unit_Price, 0) * COALESCE (Sale_Detail.Sale_Detail_Quantity, 0)) AS Stock_Sales_R FROM Sale_Detail INNER JOIN Sale_Header ON Sale_Detail.Sale_Detail_Sale_Id = Sale_Header.Sale_Id WHERE (Sale_Detail.Sale_Detail_isService = 0) AND (MONTH(Sale_Header.Sale_Date) = @MONTH) AND (YEAR(Sale_Header.Sale_Date) = @YEAR) GROUP BY Sale_Header.Sale_Date" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT A.Sale_Date, COALESCE (A.Stock_Sales_R, 0) AS Stock_Sales_R, COALESCE (B.Service_Sales_R, 0) AS Service_Sales_R
FROM     (SELECT Sale_Header.Sale_Date, SUM(COALESCE (Sale_Detail.Sale_Detail_Unit_Price, 0) * COALESCE (Sale_Detail.Sale_Detail_Quantity, 0)) AS Stock_Sales_R
                  FROM      Sale_Detail INNER JOIN
                                    Sale_Header ON Sale_Detail.Sale_Detail_Sale_Id = Sale_Header.Sale_Id
                  WHERE   (Sale_Detail.Sale_Detail_isService = 0) AND (MONTH(Sale_Header.Sale_Date) = @MONTH) AND (YEAR(Sale_Header.Sale_Date) = @YEAR)
                  GROUP BY Sale_Header.Sale_Date) AS A LEFT OUTER JOIN
                      (SELECT Sale_Header_1.Sale_Date, SUM(COALESCE (Sale_Detail_1.Sale_Detail_Unit_Price, 0) * COALESCE (Sale_Detail_1.Sale_Detail_Quantity, 0)) AS Service_Sales_R
                       FROM      Sale_Detail AS Sale_Detail_1 INNER JOIN
                                         Sale_Header AS Sale_Header_1 ON Sale_Detail_1.Sale_Detail_Sale_Id = Sale_Header_1.Sale_Id
                       WHERE   (Sale_Detail_1.Sale_Detail_isService = 1) AND (MONTH(Sale_Header_1.Sale_Date) = @MONTH) AND (YEAR(Sale_Header_1.Sale_Date) = @YEAR)
                       GROUP BY Sale_Header_1.Sale_Date) AS B ON A.Sale_Date = B.Sale_Date
GROUP BY A.Sale_Date, A.Stock_Sales_R, B.Service_Sales_R
ORDER BY A.Sale_Date</CommandText>
